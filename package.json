{
  "name": "ngx-lite",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "npm run lint && npm run prettier && npm run package && ng test @ngx-lite/all --watch=false && ng build --configuration production",
    "test": "ng test @ngx-lite/all --watch=false",
    "test:watch": "ng test @ngx-lite/all --watch --code-coverage",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "deploy": "ng build --configuration production && firebase deploy",
    "coverage": "cd coverage && lite-server",
    "prettier": "run-s prettier:*",
    "prettier:scss": "prettier --parser=scss --single-quote --write \"**/*.scss\"",
    "prettier:typescript": "prettier --parser=typescript --single-quote --write \"**/*.ts\"",
    "package": "run-s package:*",
    "package:util": "ng build @ngx-lite/util --configuration production",
    "package:carbon-ad": "ng build @ngx-lite/carbon-ad --configuration production",
    "package:code-example": "ng build @ngx-lite/code-example --configuration production",
    "package:debounce-click": "ng build @ngx-lite/debounce-click --configuration production",
    "package:eq": "ng build @ngx-lite/eq --configuration production",
    "package:in-viewport": "ng build @ngx-lite/in-viewport --configuration production",
    "package:input-datepicker": "ng build @ngx-lite/input-datepicker --configuration production",
    "package:input-timepicker": "ng build @ngx-lite/input-timepicker --configuration production",
    "package:input-range": "ng build @ngx-lite/input-range --configuration production",
    "package:input-star-rating": "ng build @ngx-lite/input-star-rating --configuration production",
    "package:input-switch": "ng build @ngx-lite/input-switch --configuration production",
    "package:input-tag": "ng build @ngx-lite/input-tag --configuration production",
    "package:json-ld": "ng build @ngx-lite/json-ld --configuration production",
    "package:loaders": "ng build @ngx-lite/loaders --configuration production",
    "package:modal": "ng build @ngx-lite/modal --configuration production",
    "package:nav-drawer": "ng build @ngx-lite/nav-drawer --configuration production",
    "package:progress": "ng build @ngx-lite/progress --configuration production",
    "package:tabs": "ng build @ngx-lite/tabs --configuration production",
    "package:menus": "ng build @ngx-lite/menus --configuration production"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.0",
    "@angular/common": "^15.2.0",
    "@angular/compiler": "^15.2.0",
    "@angular/core": "^15.2.0",
    "@angular/forms": "^15.2.0",
    "@angular/platform-browser": "^15.2.0",
    "@angular/platform-browser-dynamic": "^15.2.0",
    "@angular/router": "^15.2.0",
    "@types/prismjs": "^1.16.2",
    "blueprint-css": "^3.1.1",
    "date-fns": "^2.16.1",
    "normalize.css": "^8.0.0",
    "prismjs": "^1.14.0",
    "rxjs": "~7.5.7",
    "tslib": "^2.2.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.0",
    "@angular-eslint/builder": "15.2.1",
    "@angular-eslint/eslint-plugin": "15.2.1",
    "@angular-eslint/eslint-plugin-template": "15.2.1",
    "@angular-eslint/schematics": "15.2.1",
    "@angular-eslint/template-parser": "15.2.1",
    "@angular/cli": "^15.2.0",
    "@angular/compiler-cli": "^15.2.0",
    "@types/jasmine": "4.3.1",
    "@typescript-eslint/eslint-plugin": "5.52.0",
    "@typescript-eslint/parser": "5.52.0",
    "eslint": "8.34.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsdoc": "40.0.0",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "jasmine-core": "4.5.0",
    "jasmine-spec-reporter": "7.0.0",
    "karma": "6.4.1",
    "karma-chrome-launcher": "3.1.1",
    "karma-coverage": "2.2.0",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.0.0",
    "ng-packagr": "15.2.2",
    "npm-run-all": "4.1.2",
    "prettier": "2.8.4",
    "protractor": "7.0.0",
    "ts-node": "8.3.0",
    "typescript": "4.9.5"
  }
}
